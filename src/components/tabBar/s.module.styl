.container
  justify-content space-between
  align-items center
  padding 0 15px
  grid-area tabBar
  z-index 50

.tabBar
  display flex
  justify-content space-between
  align-items center
  padding: 10px 13px;
  line-height 6px
  box-shadow 0 -1px 25px -16px rgba(0, 0, 0, 0.1)
  background-color #1E1E1E
  border-radius: 40px